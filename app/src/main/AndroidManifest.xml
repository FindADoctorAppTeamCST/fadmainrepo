<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bt.gov.jdwnrh.www.findadoctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/jdwnrh_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jdwnrh_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:icon="@drawable/jdwnrh_logo">

            <!--
                <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            -->
        </activity>




        <activity android:name=".Help"></activity>
        <activity android:name=".setting" />
        <activity android:name=".feature" />
        <activity android:name=".More"></activity>

        <activity android:name=".AboutUs"
            android:label="About"
            android:icon="@android:drawable/ic_dialog_info">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".Feedback"
            android:label="Feedback"
            android:icon="@android:drawable/ic_menu_edit">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".RateUs"
            android:label="@string/rate_app"
            android:icon="@android:drawable/star_big_off">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".doc_login"
            android:label="@string/login"
            android:icon="@android:drawable/ic_lock_lock">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest>